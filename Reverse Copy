public class Main
{
	public static void main(String[] args) {
	    int a[]={3,7,5,8,4,2,1};
	    int b[]=new int[11];
	    for(int i=0;i<a.length;i++)
	    {
	        b[i]=a[i];
	        
	        
	    }
	    for(int j=b.length-1;j>=0;j--)
	        {
	            System.out.print(" "+b[j]);
	        
	        }
	    
		
	}
}
