public class Main
{
	public static void main(String[] args) {
	    int A[]={12,43,5,6,76,2};
	    int B[]= new int[2*A.length];
	    for(int i=0;i<A.length-1;i++){
	        B[i]=A[i];
	    }
	    A=B;
	    B=null;
	    for(int i=0;i<A.length-1;i++){
	        System.out.print(" "+A[i]);
	        
	    }
		
	}
}
